{
    "name": "elijahcruz/lari",
    "description": "A Laravel installer, but also more.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Elijah Cruz",
            "email": "ecwebservices@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel-zero/framework": "^9.0",
        "nadar/php-composer-reader": "^1.3",
        "nunomaduro/laravel-console-menu": "^3.2",
        "nunomaduro/termwind": "^1.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["lari"]
}
